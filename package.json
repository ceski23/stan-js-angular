{
	"name": "@ceski23/stan-js-angular",
	"author": "Cezary Bober",
	"license": "MIT",
	"version": "1.1.0",
	"repository": "https://github.com/ceski23/stan-js-angular",
	"description": "",
	"type": "module",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.cts",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepare": "husky",
		"build": "tsdown",
		"check:config": "attw --pack . && publint",
		"test": "bun test",
		"lint": "dprint check",
		"lint:fix": "dprint fmt",
		"prepublishOnly": "bun install && bun run build"
	},
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"@angular/core": ">=17",
		"@angular/common": ">=17",
		"stan-js": ">=1.2.0"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "0.18.2",
		"@types/bun": "1.2.20",
		"dprint": "0.50.1",
		"husky": "9.1.7",
		"publint": "0.3.12",
		"tsdown": "0.14.2",
		"typescript": "5.9.2"
	}
}
